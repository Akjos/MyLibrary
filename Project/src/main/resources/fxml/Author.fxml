<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.akjos.myLibrary.controller.AuthorController">
   <children>
      <Label text="%author.title">
         <font>
            <Font size="23.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Label>
      <TableView fx:id="table" prefWidth="560.0">
        <columns>
          <TableColumn fx:id="name" prefWidth="75.0" text="%author.table.name" />
          <TableColumn fx:id="surname" prefWidth="75.0" text="%author.table.surname" />
            <TableColumn fx:id="nickname" prefWidth="75.0" text="%author.table.nickname" />
            <TableColumn fx:id="dob" prefWidth="73.0" text="%author.table.dob" />
            <TableColumn fx:id="nationality" prefWidth="150.0" text="%author.table.nationality" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
